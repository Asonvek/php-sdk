{
  "name": "src/php-sdk",
  "type": "sdk",
  "description": "Sendios PHP SDK",
  "keywords": [
    "sendios",
    "sdk"
  ],
  "homepage": "https://sendios.io",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Sendios",
      "homepage": "https://sendios.io"
    }
  ],
  "require": {
