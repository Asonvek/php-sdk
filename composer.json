    "php": ">7.2",
    "ext-openssl": "*",
    "ext-mbstring": "*",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Sendios\\": "src/"
    }
